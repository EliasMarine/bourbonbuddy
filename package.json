{
  "name": "bourbon-buddy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "// Development scripts": "",
    "// Use dev for regular development without real-time features": "",
    "dev": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/pre-startup.ts && next dev --turbo",
    "// Use dev:realtime for WebSockets, chat, and streaming features": "",
    "dev:realtime": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' scripts/pre-startup.ts && node server.js",
    "// Build and deployment scripts": "",
    "build": "next build",
    "start": "ts-node scripts/pre-startup.ts && NODE_ENV=production node server.js",
    "start:next": "ts-node scripts/pre-startup.ts && next start",
    "// Other utilities": "",
    "lint": "next lint",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "db:backup": "ts-node scripts/backup-database.ts",
    "db:restore": "ts-node scripts/restore-database.ts",
    "db:verify": "ts-node scripts/verify-database.ts",
    "db:check": "ts-node scripts/check-db.ts",
    "db:migrate": "ts-node scripts/safe-migrate.ts",
    "db:cron-backup": "ts-node scripts/cron-backup.ts"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth/core": "^0.38.0",
    "@auth/prisma-adapter": "^2.8.0",
    "@aws-sdk/client-s3": "^3.779.0",
    "@aws-sdk/s3-request-presigner": "^3.779.0",
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@giphy/js-fetch-api": "^5.6.0",
    "@giphy/react-components": "^10.0.1",
    "@hookform/resolvers": "^4.1.3",
    "@prisma/client": "^6.5.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "bcryptjs": "^3.0.2",
    "cheerio": "^1.0.0",
    "clsx": "^2.1.1",
    "emoji-mart": "^5.6.0",
    "express-rate-limit": "^7.5.0",
    "framer-motion": "^12.6.2",
    "lucide-react": "^0.484.0",
    "next": "^15.2.4",
    "next-auth": "^4.24.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.55.0",
    "react-hot-toast": "^2.5.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.2",
    "styled-components": "^6.1.16",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.13.13",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.17",
    "postcss": "^8.4.35",
    "prisma": "^6.5.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
